<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".view.HomeScreen">

    <!--<com.google.android.material.appbar.AppBarLayout-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_width="match_parent"-->
            <!--android:theme="@style/AppBarOverlay">-->

        <androidx.appcompat.widget.Toolbar
                android:id="@+id/homScreen_toolbar"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:background="@color/colorPrimary"
                app:popupTheme="@style/PopupOverlay"/>

    <!--</com.google.android.material.appbar.AppBarLayout>-->

    <include layout="@layout/homescreen_content" android:id="@+id/include"/>
    <TextView
            android:text="@string/app_name"
            android:textSize="35sp"
            android:textColor="#000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView" app:layout_anchorGravity="top|center"
            app:layout_anchor="@+id/include"/>

</androidx.coordinatorlayout.widget.CoordinatorLayout>